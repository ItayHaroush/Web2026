{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Accept-Ranges",
          "value": "none"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Accept-Ranges",
          "value": "none"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
